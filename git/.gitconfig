	name = Lucas DRAESCHER
	email = lucas.draescher@gmail.com
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true    # use n and N to move between diff sections
	dark = true
	line-numbers = true
[merge]
	conflictstyle = diff3
[diff]
	colorMoved = default
